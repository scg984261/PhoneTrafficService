<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name ="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
	</configSections>
	<log4net>
		<appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %-5level %logger - %message%newline"></conversionPattern>
			</layout>
		</appender>
		<appender name="FileAppender" type="log4net.Appender.FileAppender">
			<file type="log4net.Util.PatternString">
				<conversionPattern value="Logs/Example-%date{yyyy-MM-dd}.log"></conversionPattern>
			</file>
			<appendToFile value ="true" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %-5level %logger - %message%newline"></conversionPattern>
			</layout>
		</appender>
		<root>
			<level value="INFO" />
			<appender-ref ref="ConsoleAppender" />
			<appender-ref ref="FileAppender" />
		</root>
	</log4net>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<appSettings>
		<add key="IncomingFilePath" value="C:\ELEPHANT\INCOMING.CSV" />
		<add key="PhoneNumbersAllocatedSpreadsheet" value="" />
	</appSettings>
